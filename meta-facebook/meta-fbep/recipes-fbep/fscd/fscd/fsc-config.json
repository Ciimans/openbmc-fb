{
  "version": "fbep-dvt-r05",
  "pwm_transition_value": 70,
  "pwm_boost_value": 100,
  "sample_interval_ms": 1000,
  "fan_recovery_time": 1,
  "boost": {
    "fan_fail": true,
    "sensor_fail": true
  },
  "sensor_valid_check": {
    "(.*)pax": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "mb_gpu": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "pdb": {
      "attribute": {
        "type" : "power_status"
      }
    }
  },
  "watchdog": false,
  "fanpower": true,
  "min_rpm": 1500,
  "profiles": {
    "linear_inlet": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 1,
      "data": [
        [23, 10],
        [28, 15],
        [32, 25],
        [37, 35],
        [39, 45],
        [40, 55],
        [42, 65],
        [45, 80]
      ]
    },
    "linear_switch_remote": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [60, 20],
        [65, 25],
        [70, 30],
        [75, 35],
        [80, 40],
        [90, 70]
      ]
    },
    "linear_gpu_edge": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 3,
      "data": [
        [50, 20],
        [55, 35],
        [60, 45],
        [65, 50],
        [68, 55],
        [70, 60]
      ]
    },
    "pid_gpu_edge": {
      "read_source" : {
	"util" : "/usr/local/bin/sensor-util"
      },
      "type": "pid",
      "setpoint": 71,
      "positive_hysteresis": 2,
      "negative_hysteresis": 1,
      "kp": -3.5,
      "ki": -0.4,
      "kd": -0.4
    },
    "linear_gpu_mem": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 3,
      "data": [
        [62, 20],
        [65, 45],
        [68, 50],
        [70, 55],
        [73, 60],
        [75, 65]
      ]
    },
    "pid_gpu_mem": {
      "read_source" : {
	"util" : "/usr/local/bin/sensor-util"
      },
      "type": "pid",
      "setpoint": 76,
      "positive_hysteresis": 2,
      "negative_hysteresis": 2,
      "kp": -3.5,
      "ki": -0.4,
      "kd": -0.4
    },
    "linear_vicor": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 2,
      "data": [
        [60, 20],
        [65, 25],
        [70, 30],
        [75, 35],
        [80, 40],
        [90, 70]
      ]
    }
  },
  "fans": {
    "0": {
      "label" : "Fan 0 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 0"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 0"
      }
    },
    "1": {
      "label" : "Fan 0 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 1"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 0"
      }
    },
    "2": {
      "label" : "Fan 1 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 2"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 1"
      }
    },
    "3": {
      "label" : "Fan 1 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 3"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 1"
      }
    },
    "4": {
      "label" : "Fan 2 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 4"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 2"
      }
    },
    "5": {
      "label" : "Fan 2 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 5"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 2"
      }
    },
    "6": {
      "label" : "Fan 3 Inlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 6"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 3"
      }
    },
    "7": {
      "label" : "Fan 3 Outlet",
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 7"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 3"
      }
    }
  },
  "zones": {
    "zone_1": {
      "pwm_output": [0, 2, 4, 6],
      "expr_file": "zone1.fsc"
    }
  }
}
